{
	"schemaVersion": 1,
	"id": "flourishing_fields",
	"version": "${version}",
	"name": "Flourishing Fields",
	"description": "A Minecraft fabric mod about flowers and bees",
	"authors": [
		"q4niel"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/flourishing-fields",
		"sources": "https://github.com/q4niel/Flourishing-Fields"
	},
	"license": "Flourishing Fields License 1.0",
	"icon": "assets/flourishing_fields/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "dev.q4niel.flourishing_fields.FlourishingFields",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "dev.q4niel.flourishing_fields.FlourishingFieldsDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"flourishing_fields.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.13",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}